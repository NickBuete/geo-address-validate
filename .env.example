# Copy to .env and fill in your Geoscape consumer key
GEOSCAPE_CONSUMER_KEY=your_consumer_key_here
# Optional: if your tenant exposes a separate API key token, set GEOSCAPE_API_KEY as a fallback.
# GEOSCAPE_API_KEY=your_api_key_here
# Optional overrides for legacy tenants or different params:
# GEOSCAPE_PREDICT_URL=https://api.psma.com.au/v1/predictive/address
# GEOSCAPE_ADDRESS_URL=https://api.psma.com.au/v1/predictive/address
# GEOSCAPE_PREDICT_QUERY_PARAM=query

# New Zealand (NZ Post) predictive address configuration
# NZPOST_BEARER_TOKEN=manual_bearer_token_override
# NZPOST_CLIENT_ID=your_client_id_here
# NZPOST_CLIENT_SECRET=your_client_secret_here
# NZPOST_USER_NAME=optional_username
# NZPOST_BASE_URL=https://api.nzpost.co.nz/addresschecker/1.0
# NZPOST_PARCEL_BASE_URL=https://api.nzpost.co.nz/parceladdress/2.0
# NZPOST_DEFAULT_TYPE=All
# NZPOST_MAX_RESULTS=10
# NZPOST_ACCEPT=application/json
# NZPOST_SCOPE=            # optional; leave blank unless NZ Post assigns a scope
# NZPOST_TOKEN_URL=https://oauth.nzpost.co.nz/as/token.oauth2
# NZPOST_AU_COUNTRY=australia

PORT=4000
